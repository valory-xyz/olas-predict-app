{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx"],
        "alias": {
          "utils": "./utils",
          "components": "./components",
          "images": "./public/images",
          "hooks": "./hooks",
          "types": "./types",
          "constants": "./constants"
        }
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    ["inline-react-svg"]
  ]
}
